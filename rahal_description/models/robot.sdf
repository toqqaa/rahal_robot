    <joint name='front_left_wheel_joint' type='revolute'>
                <pose relative_to='front_left_wheel'/>

      <parent>base_link</parent>
      <child>front_left_wheel</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
      <limit>
        <effort>10</effort>
        <velocity>10</velocity>
        <lower>-1.57</lower>
        <upper>1.57</upper>
      </limit>
    </joint>

