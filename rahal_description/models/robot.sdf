    </joint>





    <link name='camera_link'>
  <pose relative_to='base_link'>0.28252 0.0 0.037 0 -0.35 0</pose>
  <inertial>
    <pose>0 0 0 0 0 0</pose>
    <mass>0.15</mass>
    <inertia>
      <ixx>0.00015</ixx>
      <ixy>0</ixy>
      <ixz>0</ixz>
      <iyy>0.00015</iyy>
      <iyz>0</iyz>
      <izz>0.00015</izz>
    </inertia>
  </inertial>
  <visual name='cam_visual'>
    <pose>0 0 0 0 0 0</pose>
    <geometry>
      <box>
        <size>0.02 0.06 0.02</size>
      </box>
    </geometry>
    <material>
      <diffuse>0.2 0.2 0.2 1.0</diffuse>
      <ambient>0.2 0.2 0.2 1.0</ambient>
    </material>
  </visual>
  <collision name='cam_collision'>
